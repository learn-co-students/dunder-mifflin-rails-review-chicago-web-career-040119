<h1>Dog Form</h1>

<% if @errors %>
  <% @errors.each do |error| %>
    <%=  error %>
  <% end %>
<% end %>


<%= form_for @dog do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :breed %><br>
  <%= f.text_field:breed %><br>

  <%= f.label :age%><br>
  <%= f.text_field :age%><br>

  <%= f.submit %>
<% end %>
